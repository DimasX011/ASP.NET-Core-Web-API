<Window x:Class="WpfClientUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfClientUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <local:MatherialCards Grid.Column="0" Grid.Row="0" x:Name="CpuChart">
            </local:MatherialCards >
            <Button Height="46"
            Click="Button_Click" Margin="196,0,194,0" Content="Замер"/>
            
            <local:GpuControl Grid.Column="10" Grid.Row="15" x:Name="GpuChart">
            </local:GpuControl >
            <Button Height="46"
            Click="Button_Click" Margin="196,-20,194,0" Content="Замер"/>
            
            
        </StackPanel>
    </Grid>
</Window>
